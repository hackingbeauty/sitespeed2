%h2
  Alexa Top 100 Sites
= will_paginate @urls
%table#top100
  %thead
    %th.alexa
      US Rank
    %th
      URL
    %th.yslow
      Yslow
    %th.page_speed
      Page Speed
  %tbody
    - @urls.each do |u|
      %tr{:class=>"#{cycle('odd', 'even')} row"}
        %td.center
          = u.country_rank
        %td.url      
          = link_to u.url, u
        %td.center
          = (u.yslow2s.find(:last) == nil) ? 'N/A' : u.yslow2s.find(:last).o
        %td.center
          = (u.page_speeds.find(:last) == nil) ? 'N/A' : u.page_speeds.find(:last).o



